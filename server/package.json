{
  "name": "react-electron-editor",
  "version": "1.0.0",
  "scripts": {
    "start-client": "cd ../client && yarn start",
    "server": "node server.js",
    "start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn start-client\""
  },
  "dependencies": {
    "express": "^4.16.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
