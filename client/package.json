{
  "name": "react-electron-editor",
  "description": "View & modify local files using React & Electron backed by an Express server",
  "version": "1.0.0",
  "author": "LauraAubin",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "electron-build": "electron-builder",
    "start-electron": "electron .",
    "start-app": "concurrently \"BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && yarn start-electron\"",
    "build-app": "yarn react-build && yarn electron-build"
  },
  "repository": "https://github.com/LauraAubin/react-electron-editor",
  "license": "ISC",
  "dependencies": {
    "electron-is-dev": "0.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "electron": "^6.0.2",
    "electron-builder": "^21.2.0",
    "concurrently": "^3.5.0",
    "wait-on": "^3.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000/"
}
